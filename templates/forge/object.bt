#ifndef FORGE_OBJECT
#define FORGE_OBJECT

#include "common.bt"

typedef struct
{    
    int revision; // 10 (DCS/FME)
    int always_8;

    uint file_count; // Uusally 0
    if (file_count == 0) return;
    
    struct File
    {
        FString name;
        uint64 size;
        byte data[size];
    } files[file_count]<read=this.name.text, optimize=false>;
} Object;

#endif